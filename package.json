{
	"name": "zation",
	"displayName": "Zation",
	"publisher": "LucaCode",
	"description": "Snippets and commands for Zation development.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ZationServer/zation-vscode.git"
	},
	"icon": "assets/icon.png",
	"license": "MIT",
	"categories": [
		"Snippets",
		"Other"
	],
	"keywords": [
		"Zation",
		"Zation Server",
		"Application Server Framework",
		"Real-Time",
		"Fast"
	],
	"activationEvents": [
		"onCommand:zation.NewProject",
		"onCommand:zation.CloneClusterComponent",
		"onCommand:zation.ShowVersions"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "zation.NewProject",
				"title": "Zation: New Project"
			},
			{
				"command": "zation.CloneClusterComponent",
				"title": "Zation: Clone Cluster Component"
			},
			{
				"command": "zation.ShowVersions",
				"title": "Zation: Show Versions"
			}
		],
		"snippets": [
			{
				"language": "typescript",
				"path": "./snippets/typescriptSnippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"fs-extra": "8.1.0",
		"download-git-repo": "3.0.2"
	},
	"devDependencies": {
		"@types/fs-extra": "^8.0.1",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
